    "gulp-util": "^3.0.8",
    "jquery": "^3.4.1",
    "laravel-mix": "^2.0",
    "lodash": "^4.17.15",
    "node-sass": "^4.13.1",
    "popper.js": "^1.12",
    "vue": "^2.5.7"
  },
